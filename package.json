{
	"name": "@arkecosystem/multisig-server",
	"version": "2.1.0",
	"description": "A RESTful API to interact with Multi-Signatures on the ARK Blockchain.",
	"license": "MIT",
	"author": "Brian Faust <brian@ark.io>",
	"files": [
		"/bin",
		"/dist",
		"/oclif.manifest.json"
	],
	"main": "dist/index",
	"types": "dist/index",
	"bin": {
		"musig": "./bin/run"
	},
	"scripts": {
		"build": "yarn clean && yarn compile",
		"build:watch": "yarn clean && yarn compile -w",
		"clean": "del dist",
		"compile": "./node_modules/typescript/bin/tsc",
		"multisig-server": "./bin/run",
		"format": "yarn lint && yarn prettier",
		"lint": "eslint ./src/** --ext .ts --fix",
		"lint:tests": "eslint ./__tests__/** --ext .ts --fix",
		"prettier": "prettier --write \"./*.{ts,js,json,md}\" \"./**/*.{ts,js,json,md}\"",
		"prepublishOnly": "yarn build",
		"test": "jest --forceExit --runInBand"
	},
	"dependencies": {
		"@arkecosystem/core-magistrate-crypto": "^3.0.0",
		"@arkecosystem/crypto": "^3.0.0",
		"@hapi/boom": "^9.1.0",
		"@hapi/hapi": "^20.0.1",
		"@kodekeep/foreman": "^1.0.0",
		"@oclif/command": "^1.8.0",
		"@oclif/config": "^1.17.0",
		"@oclif/plugin-autocomplete": "^0.2.0",
		"@oclif/plugin-commands": "^1.3.0",
		"@oclif/plugin-help": "^3.2.0",
		"@oclif/plugin-not-found": "^1.2.4",
		"@oclif/plugin-plugins": "^1.9.1",
		"@types/eslint": "^7.2.4",
		"@types/eslint-plugin-prettier": "^3.1.0",
		"@typescript-eslint/eslint-plugin": "^4.6.1",
		"@typescript-eslint/parser": "^4.6.1",
		"better-sqlite3": "^7.1.1",
		"chalk": "^4.1.0",
		"clear": "^0.1.0",
		"cli-table3": "^0.6.0",
		"cli-ux": "^5.5.0",
		"dayjs": "^1.9.5",
		"env-paths": "^2.2.0",
		"eslint": "^7.13.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-jest": "^24.1.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-simple-import-sort": "^5.0.3",
		"execa": "^4.1.0",
		"fast-levenshtein": "^3.0.0",
		"fs-extra": "^9.0.1",
		"get-value": "^3.0.1",
		"got": "^11.8.0",
		"joi": "^17.6.0",
		"latest-version": "^5.1.0",
		"lodash.minby": "^4.6.0",
		"media-type": "^0.3.1",
		"micromatch": "^4.0.2",
		"nanomatch": "^1.2.13",
		"nodejs-tail": "^1.1.0",
		"pino": "^6.7.0",
		"pino-pretty": "^4.3.0",
		"pretty-bytes": "^5.4.1",
		"pretty-ms": "^7.0.1",
		"prompts": "^2.4.0",
		"rate-limiter-flexible": "^2.1.13",
		"read-last-lines": "^1.7.2",
		"semver": "^7.3.2",
		"set-value": "^4.1.0",
		"uuid": "^8.3.1",
		"wif": "^2.0.6"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1.22.2",
		"@sindresorhus/tsconfig": "^0.7.0",
		"@types/execa": "^2.0.0",
		"@types/fast-levenshtein": "^0.0.1",
		"@types/fs-extra": "^9.0.3",
		"@types/got": "^9.6.11",
		"@types/hapi__boom": "^9.0.1",
		"@types/hapi__hapi": "^20.0.2",
		"@types/jest": "^26.0.15",
		"@types/lodash.minby": "^4.6.6",
		"@types/lodash.sample": "^4.2.6",
		"@types/node": "^14.14.6",
		"@types/pino": "^6.3.3",
		"@types/prettier": "^2.1.5",
		"@types/pretty-bytes": "^5.2.0",
		"@types/pretty-ms": "^5.0.1",
		"@types/prompts": "^2.4.0",
		"@types/rimraf": "^3.0.0",
		"@types/semver": "^7.3.4",
		"@types/uuid": "^8.3.0",
		"@types/wif": "^2.0.2",
		"codecov": "^3.8.1",
		"cross-env": "^7.0.2",
		"del-cli": "^3.0.1",
		"jest": "^26.6.3",
		"jest-extended": "^0.11.5",
		"prettier": "^2.1.2",
		"rimraf": "^3.0.2",
		"ts-jest": "^26.4.4",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.5"
	},
	"engines": {
		"node": ">=12.x"
	},
	"publishConfig": {
		"access": "public"
	},
	"oclif": {
		"commands": "./dist/commands",
		"hooks": {
			"init": [
				"./dist/hooks/init/update"
			],
			"command_not_found": [
				"./dist/hooks/command_not_found/suggest"
			]
		},
		"bin": "musig",
		"plugins": [
			"@oclif/plugin-autocomplete",
			"@oclif/plugin-commands",
			"@oclif/plugin-help"
		]
	}
}
